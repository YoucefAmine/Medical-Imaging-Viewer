{
  "rewrites": [
    { "source": "/DICOM_Visionneuse/**", "destination": "/DICOM_Visionneuse/index.html" }
  ],
  "headers": [
    {
      "source": "/DICOM_Visionneuse/**",
      "headers": [
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
      ]
    }
  ]
}
